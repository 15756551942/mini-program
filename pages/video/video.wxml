<view class="videoContainer">
  <view class="header">
    <image src="/static/images/video/video.jpg"></image>
    <view class="serach">搜索音乐</view>
    <image src="/static/images/logo.png"></image>
  </view>
  <scroll-view scroll-x class="navScroll" enable-flex>
    <view class="navItem" wx:for="{{videoGroupList}}" wx:key="id">
      <view class="navContent {{item.id === navId?'active':''}}" bindtap="changeNav" id="{{item.id}}">{{item.name}}</view>
    </view>
  </scroll-view>
</view>